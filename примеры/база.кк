{
    бэ = 10 + 1.
    воспевай 2*2 + 2.
    воспевай "\n".
    воспевай ((-бэ)).
    воспевай "\n".
    дабы (бэ==бэ) {
        воспевай ("Слава Руси!"+"\n").
    } иначе {
        воспевай ("Черная магия!"+"\n").
    }.
    ящеры = 4.
    дабы ящеры == 0 {
        воспевай ("Слава Руси!"+"\n").
    } иначе {
        воспевай ("Изыдите ящеры"+"\n").
    }.
    доколе ящеры > 0 {
        воспевай "Держись, Русь-Матушка, ящеров "+(str)ящеры+" осталось!\n".
        ящеры = ящеры - 1.
    }.
    воспевай "Слава Руси! Ящеры изгнаны! Слава Перуну!".
}