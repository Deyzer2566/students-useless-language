{
    первый = 0.
    второй = 1.
    количество_выводимых_элементов = 10.
    счётчик = 2.
    воспевай "1 число Фибоначчи = "+(str)(первый + второй)+"\n".
    доколе счётчик < количество_выводимых_элементов {
        воспевай (str)счётчик+" число Фибоначчи = "+(str)(первый + второй)+"\n".
        дабы счётчик - счётчик/2*2 == 0 {
            первый = первый + второй.
        } иначе {
            второй = первый + второй.
        }.
        счётчик = счётчик + 1.
    }.
}